{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/документи/:filename(.+\\.pdf)",
      "destination": "/документи/:filename"
    },
    {
      "source": "/документи/:path*",
      "destination": "/"
    },
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/"
    }
  ],
  "cleanUrls": true
}

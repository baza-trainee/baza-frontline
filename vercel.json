{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/documents/:filename(.+\\.pdf)",
      "destination": "/documents/:filename"
    },
    {
      "source": "/documents/:path*",
      "destination": "/"
    },
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/"
    }
  ]
}
